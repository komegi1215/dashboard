{
    "manifest_version": 3,
    "name": "Personal Dashboard",
    "version": "1.0.0",
    "description": "Personal dashboard with to-do, weather, and more",
    "action": {
        "default_icon": "icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage"],
  "host_permissions": [
    "https://newsapi.org/",
    "https://apis.scrimba.com/",
    "https://api.exchangerate.host/"
  ]
}
